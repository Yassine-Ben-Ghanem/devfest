{% extends 'base.html.twig' %}

{% block title %}Feedback{% endblock %}

{% block body %}
<br><br><br><br><br><br><br><br><br><br>

    <h1>Feedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feedback.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ feedback.description }}</td>
            </tr>
            <tr>
                <th>PostedAt</th>
                <td>{{ feedback.postedAt ? feedback.postedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('feedback_index') }}">back to list</a>

    <a href="{{ path('feedback_edit', {'id': feedback.id}) }}">edit</a>

    {{ include('feedback/_delete_form.html.twig') }}
{% endblock %}
